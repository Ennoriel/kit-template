{
	"name": "kit-template",
	"version": "1.0.0",
	"private": true,
	"scripts": {
		"dev": "npm-run-all --parallel sveltekit typesafe-i18n",
		"sveltekit": "vite dev --open --host",
		"build": "vite build",
		"preview": "vite preview --open --host",
		"test": "playwright test",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "eslint .",
		"prettier": "prettier --write ./**/* --no-error-on-unmatched-pattern --ignore-unknown",
		"typesafe-i18n": "typesafe-i18n"
	},
	"devDependencies": {
		"@aws-sdk/client-s3": "^3.254.0",
		"@playwright/test": "^1.29.2",
		"@sveltejs/adapter-auto": "1.0.2",
		"@sveltejs/kit": "1.0.12",
		"@types/bcryptjs": "^2.4.2",
		"@types/cookie": "^0.5.1",
		"@types/jest": "^29.2.6",
		"@types/jsonwebtoken": "^9.0.1",
		"@types/prismjs": "^1.26.0",
		"@typescript-eslint/eslint-plugin": "^5.48.2",
		"@typescript-eslint/parser": "^5.48.2",
		"chyme": "^1.3.9",
		"chyme-svelte": "^1.3.0",
		"eslint": "^8.32.0",
		"eslint-config-prettier": "^8.6.0",
		"eslint-plugin-svelte3": "^4.0.0",
		"mongodb": "^4.13.0",
		"node-mailjet": "^6.0.1",
		"npm-run-all": "^4.1.5",
		"prettier": "^2.8.3",
		"prettier-plugin-svelte": "^2.9.0",
		"prism-svelte": "^0.5.0",
		"prismjs": "^1.29.0",
		"svelte": "^3.55.1",
		"svelte-awesome-color-picker": "^2.3.0",
		"svelte-check": "^3.0.2",
		"svelte-markdown": "^0.2.3",
		"svelte-notifications": "^0.9.98",
		"svelte-preprocess": "^5.0.1",
		"tslib": "^2.4.1",
		"typescript": "^4.9.4",
		"vite": "^4.0.4"
	},
	"type": "module",
	"dependencies": {
		"bcryptjs": "^2.4.3",
		"cookie": "^0.5.0",
		"jsonwebtoken": "^9.0.0",
		"typesafe-i18n": "^5.20.0"
	}
}
