{
	"name": "kit-template",
	"version": "1.0.0",
	"private": true,
	"scripts": {
		"dev": "npm-run-all --parallel sveltekit typesafe-i18n",
		"sveltekit": "vite dev --open --host",
		"build": "vite build",
		"preview": "vite preview --open --host",
		"test": "playwright test",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "eslint .",
		"prettier": "prettier --write ./**/* --no-error-on-unmatched-pattern --ignore-unknown",
		"typesafe-i18n": "typesafe-i18n"
	},
	"devDependencies": {
		"@aws-sdk/client-s3": "^3.204.0",
		"@playwright/test": "^1.27.1",
		"@sveltejs/adapter-auto": "1.0.0-next.87",
		"@sveltejs/kit": "1.0.0-next.535",
		"@types/bcryptjs": "^2.4.2",
		"@types/cookie": "^0.5.1",
		"@types/jest": "^29.2.2",
		"@types/jsonwebtoken": "^8.5.9",
		"@types/prismjs": "^1.26.0",
		"@typescript-eslint/eslint-plugin": "^5.42.0",
		"@typescript-eslint/parser": "^5.42.0",
		"chyme": "^1.3.5",
		"chyme-svelte": "^1.2.2",
		"eslint": "^8.26.0",
		"eslint-config-prettier": "^8.5.0",
		"eslint-plugin-svelte3": "^4.0.0",
		"mongodb": "^4.11.0",
		"node-mailjet": "^5.1.1",
		"npm-run-all": "^4.1.5",
		"prettier": "^2.7.1",
		"prettier-plugin-svelte": "^2.8.0",
		"prism-svelte": "^0.5.0",
		"prismjs": "^1.29.0",
		"svelte": "^3.52.0",
		"svelte-awesome-color-picker": "^2.1.3",
		"svelte-check": "^2.9.2",
		"svelte-markdown": "^0.2.3",
		"svelte-preprocess": "^4.10.7",
		"tslib": "^2.4.1",
		"typescript": "^4.8.4",
		"vite": "^3.2.2"
	},
	"type": "module",
	"dependencies": {
		"bcryptjs": "^2.4.3",
		"cookie": "^0.5.0",
		"jsonwebtoken": "^8.5.1",
		"typesafe-i18n": "^5.16.2"
	}
}
